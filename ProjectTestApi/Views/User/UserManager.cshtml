@using Newtonsoft.Json;
@using ProjectTestApi.Models.Const;
@using System.Net;
@{
    ViewData["Title"] = "UserManager";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<Member1Model>
@{
    string cookieUserValue = Context.Request.Cookies[MyConstanst.KeyUserNameCookies];
    string cookiePassValue = Context.Request.Cookies[MyConstanst.KeyPasswordCookies];
    string apiContent = null;
}
@*BreadCrumb*@
@{
    string apiUrlBreadCrumb = MyConstanst.SiteName
    + "web.breadcrumb.asp?id=" + Model[0].id;
    string jsonBreadCrumb = await (new WebClient()).DownloadStringTaskAsync(apiUrlBreadCrumb);
    if (!string.IsNullOrEmpty(jsonBreadCrumb))
    {
        List<BreadCrumbModel> breadCrumbModels = JsonConvert
        .DeserializeObject<List<BreadCrumbModel>>
        (jsonBreadCrumb);

        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                @foreach (var breadcrumb in breadCrumbModels)
                {
                    <li class="breadcrumb-item"><a href="/@breadcrumb.url">@breadcrumb.tieude</a></li>
                }
            </ol>
        </nav>
    }
}

