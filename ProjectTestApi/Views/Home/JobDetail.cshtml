                }
                else
                {
                    if (Model.Count > 0)
                    {
                        var urlResult = "http://ww2.tuyennhansu.vn/module." + Model[0].module + ".asp?id=" + Model[0].id;
                        var json = (new WebClient()).DownloadString(urlResult);
                        var result = JsonConvert.DeserializeObject<List<ApiModel>>(json);
                        <div class="container"></div>
                        <div class="id30032 md-timboloc ">
                            <div class="container">
                                <div class="th-tuyendung">
                                    <div class="container">
                                        @if (Model[0].tieude != null && Model[0].url != null)
                                        {
                                            <h2><a href="@Model[0].url">@Model[0].tieude</a></h2>
                                        }
                                        <div class="row">
                                            @{
                                                foreach (var item in result[0].data)
                                                {
                                                    <div class="tuyendung">
                                                        <div class="wrap-ndct">
                                                            <div class="@item.loaitin 37499 111">
                                                                <div class="ct">
                                                                    @if (@item.url != null && item.tieude != null)
                                                                    {
                                                                        <h3>
                                                                            <a href="@item.url">
                                                                                @item.tieude
                                                                            </a>
                                                                        </h3>
                                                                    }
                                                                    @if (item.tenND != null)
                                                                    {
                                                                        <span class="tenND">
                                                                            @item.tenND
                                                                        </span>
                                                                    }
                                                                    @if (item.gia != null)
                                                                    {
                                                                        <div class="gia"><span class="dv">@item.gia[0].donvi</span></div>
                                                                    }
                                                                    @if (item.diachiND != null)
                                                                    {
                                                                        <span class="diachiND">@item.diachiND</span>
                                                                    }
                                                                    @if (item.chucdanh != null)
                                                                    {
                                                                        <span class="chucdanh">
                                                                            @foreach (var cd in item.chucdanh)
                                                                            {
                                                                                <a href="@cd.url">@cd.tengoi</a>
                                                                            }
                                                                        </span>
                                                                    }
                                                                    @if (item.loai != null)
                                                                    {
                                                                        <span class="loai">
                                                                            @foreach (var type in item.loai)
                                                                            {
                                                                                <a href="@type.url">
                                                                                    @type.tengoi
                                                                                </a>
                                                                            }
                                                                        </span>
                                                                    }
                                                                    @if (item.thoihan != null)
                                                                    {
                                                                        <span class="thoihan">
                                                                            @item.thoihan
                                                                        </span>
                                                                    }
                                                                    @if (item.ngoaingu != null)
                                                                    {
                                                                        <span class="ngoaingu">
                                                                            @foreach (var language in item.ngoaingu)
                                                                            {
                                                                                <a href="@language.url">@language.tengoi</a>
                                                                            }
                                                                        </span>
                                                                    }
                                                                    @if (item.nganh != null)
                                                                    {
                                                                        <div class="vttd">
                                                                            @foreach (var career in item.nganh)
                                                                            {
                                                                                <a href="@career.url">@career.tengoi</a>
                                                                            }
                                                                        </div>
                                                                    }
                                                                    <div class="group-wishlist">
                                                                        <div class="wish_list "></div>
                                                                        <div class="add_to_cart da-ung-tuyen"></div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                }
                                            }
                                            <div class="xemthem"><span><a href="/undefined"></a></span></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                }
            }
        }catch(Exception ex)
        {
            throw;
        }
