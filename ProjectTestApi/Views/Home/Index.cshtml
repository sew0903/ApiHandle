                                                        }
                                                    </div>
                                                    <div class="vttd">
                                                        @if(data.value.nganh != null)
                                                        {
                                                            @foreach (var carrer in data.value.nganh)
                                                            {
                                                                <a href="@carrer.url">@carrer.tengoi</a>
                                                            }
                                                        }
                                                    </div>
                                                    <div class="group-wishlist">
                                                        <div class="wish_list "></div>
                                                        <div class="add_to_cart  "></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="container tt-tuyendung"></div>
                </div>
            }
            catch (Exception)
            {
                
                throw;
            }
            break;
        case 35028:
            try
            {
                <div class="id35028 ykienkhachhang">
                    <div class="container">
                        <h2>
                            <a href="@item.value.url">@item.value.tieude</a>
                        </h2>
                    </div>
                </div>
            }
            catch (Exception)
            {
                
                throw;
            }
            break;
        case 35030:
            try
            {
                <div class="id35030 laytimkiem">
                    <div class="container">
                        <h2>
                            <a href="@item.value.url">@item.value.tieude</a>
                        </h2>
                    </div>
                    <div class="th-laytimkiem">
                        <div class="container">
                            <form class="search-congviec">
                                <div class="item item-search">
                                    <input type="text" class="input-timcongviec" value="">
                                </div>
                                <div class="item item-city">
                                    <input type="text" class="input-timcongviec" value="" data-bs-toggle="modal" data-bs-target="#exampleModal">
                                    <span class="clear-tinhthanh "></span>
                                </div>
                                <button class="btn-tk" type="button" data-bs-toggle="modal" data-bs-target="#exampleModal"></button>
                            </form>
                        </div>
                    </div>
                    <div class="container tt-laytimkiem"></div>
                </div>
            }
            catch (Exception)
            {
                
                throw;
            }
            break;
    }
}  
<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel" >Chọn theo khu vực tỉnh, thành phố </h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <select class="form-select" size="7" aria-label="size 3 select example">
                    @{
                        if(arrStates != null)
                        {
                            foreach(var state in arrStates)
                            {                               
                                        <option data-bs-dismiss="modal" class="state-value" value="@state.id" onclick="ClickStateHandle()">@state.ten</option>
                            }
                        }
                    }
                </select>
            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>


