    ViewData["Title"] = "Trang chủ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    var urlData = "http://ww2.tuyennhansu.vn/module.Tuyendung.asp?id=35004&pageid=1";
}
